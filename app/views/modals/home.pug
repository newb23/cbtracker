#modal-add-account.modal.fade(tabindex='-1' role='dialog' style='display: none;' aria-hidden='true')
  .modal-dialog.modal-dialog-centered(role="document")
    .modal-content.bg-dark
      .modal-header
        h5.modal-title.text-white
          | Add a account
        button(type="button", class="btn-close", data-bs-dismiss="modal", aria-label="Close")
      .modal-body
        form
          .form-group.mb-3
            input#inp-name.form-control.form-control.bg-dark(type='text' placeholder='Account Name')
          .form-group.mb-3
            input#inp-address.form-control.form-control.bg-dark(type='text' placeholder='Address')
